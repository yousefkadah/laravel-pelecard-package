{
    "name": "yousefkadah/laravel-pelecard",
    "description": "Laravel package for Pelecard payment gateway integration with Cashier-compatible subscription billing and multi-tenancy support",
    "keywords": [
        "laravel",
        "pelecard",
        "payment",
        "gateway",
        "subscription",
        "billing",
        "cashier",
        "multi-tenant"
    ],
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Yousef Kadah",
            "email": "yousefkadah@gmail.com"
        }
    ],
    "require": {
        "php": "^8.1|^8.2|^8.3",
        "illuminate/support": "^10.0|^11.0|^12.0",
        "illuminate/database": "^10.0|^11.0|^12.0",
        "illuminate/contracts": "^10.0|^11.0|^12.0",
        "guzzlehttp/guzzle": "^7.5"
    },
    "require-dev": {
        "orchestra/testbench": "^8.0|^9.0",
        "phpunit/phpunit": "^10.0|^11.0",
        "laravel/pint": "^1.13",
        "rector/rector": "^1.0",
        "phpstan/phpstan": "^1.10",
        "larastan/larastan": "^2.9"
    },
    "autoload": {
        "psr-4": {
            "Yousefkadah\\Pelecard\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Yousefkadah\\Pelecard\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Yousefkadah\\Pelecard\\PelecardServiceProvider"
            ],
            "aliases": {
                "Pelecard": "Yousefkadah\\Pelecard\\Facades\\Pelecard"
            }
        }
    },
    "scripts": {
        "post-autoload-dump": [
            "@php vendor/bin/testbench package:discover --ansi"
        ],
        "dev": [
            "@pint",
            "@rector",
            "@phpstan",
            "@test"
        ],
        "pint": "pint",
        "rector": "rector process",
        "phpstan": "phpstan analyse --memory-limit=2G",
        "test": "vendor/bin/phpunit"
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
